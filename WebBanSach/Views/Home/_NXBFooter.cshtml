@model IEnumerable<WebBanSach.Models.NhaXuatBan>

<div class="item__wrap">
    <h3 class="item__title">Nhà xuất bản tiêu biểu</h3>
    <div class="item__content">
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="@Url.Action("Index",new {@nxb = item.MaNXB})">@item.TenNXB</a></li>
            }
        </ul>
    </div>
</div>
